		<name><![CDATA[全院在院患者查询统计表]]></name>
		<dataSets>
			<dataSet name="ds" type="DB_SQL" >
				<property name="dataSource">JHEMR_DS</property>
				<property name="desc"><![CDATA[]]></property>
				<property name="query"><![CDATA[select p.patient_id as 住院号,m.name 姓名,m.sex 性别,p.age as 年龄,b.bed_label 床号,m.phone_number_home as 联系电话,p.doctor_in_charge as 管床医师,d.diagnosis_desc as 主诊断,p.admission_date_time as 入院日期,dd.dept_name as 入院科别,p.admission_date_time as 入院日期,floor(p.discharge_date_time-p.admission_date_time)+1 as 住院天数,p.mailing_address as 现住址  
from (select * from pat_visit pp where pp.discharge_date_time is  null and pp.admission_date_time>? and pp.admission_date_time<? and pp.dept_admission_to=? order by pp.admission_date_time) p left join pat_master_index m on p.patient_id=m.patient_id left join bed_rec b on p.bed_no=b.bed_no  left join (select * from PAT_DIAGNOSIS d where d.diagnosis_note like '主诊断') d on p.patient_id=d.patient_id  left join dept_dict dd on dd.dept_code = p.DEPT_ADMISSION_TO ]]></property>
				<property name="beginRow">0</property>
				<property name="endRow">0</property>
				<queryParam name="start" />
				<queryParam name="end" />
				<queryParam name="dept_code" />
			</dataSet>
		</dataSets>
